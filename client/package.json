{
  "name": "mern-starter-kit",
  "version": "1.0.0",
  "main": "app.js",
  "description": "A fullstack MERN boilerplate.",
  "repository": "https://github.com/mattcarlotta/fullstack-mern-kit",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "initialize": "yarn install",
    "build": "better-npm-run build",
    "test": "better-npm-run test",
    "test:coverage": "better-npm-run test:coverage",
    "format": "prettier --write 'src/**/*.js'",
    "lintjs": "eslint 'src/**/*.js' --fix",
    "lintstyles": "stylelint **/*.scss",
    "pre-commit": "lint-staged"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server",
      "env": {
        "APIPORT": 5000,
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "APIPORT": 8080,
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest --config ./jest.json --watchAll",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "test:coverage": {
      "command": "jest --config ./jest.json --coverage",
      "env": {
        "NODE_ENV": "testing"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote",
      "eslint --fix",
      "jest --config ./jest.json /src",
      "git add"
    ],
    "*.scss": [
      "stylelint **/*.scss",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "better-npm-run": "^0.1.1",
    "connected-react-router": "^6.3.1",
    "history": "^4.7.2",
    "lodash": "4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-beta.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.4.2",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "error-overlay-webpack-plugin": "^0.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-watch": "^5.0.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "jest-css-modules-transform": "^2.1.1",
    "jest-enzyme": "7.0.2",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "jsdom": "^14.0.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "4.2.1",
    "webpackbar": "^3.1.5"
  }
}
